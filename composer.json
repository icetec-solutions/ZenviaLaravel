{
    "name": "louis/zenvia",
    "description": "Zenvia Package for sending SMS",
    "keywords": [
        "zenvia",
        "sms",
        "php",
        "laravel"
    ],
    "homepage": "https://github.com/tymondesigns/jwt-auth",
    "support": {
        "issues": "https://github.com/tymondesigns/jwt-auth/issues",
        "source": "https://github.com/tymondesigns/jwt-auth"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Kevin Louis",
            "email": "keviinlouiis@gmail.com",
            "homepage": "https://github.com/keviinlouis",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.0",
        "illuminate/http": "5.1.* || 5.2.* || 5.3.* || 5.4.* || 5.5.* || 5.6.*",
        "illuminate/support": "5.1.* || 5.2.* || 5.3.* || 5.4.* || 5.5.* || 5.6.*",
        "symfony/http-foundation": "~2.7|~3.0|~4.0",
        "symfony/http-kernel": "~2.7|~3.0|~4.0"
        "guzzlehttp/guzzle": "~6.0",
        "nesbot/carbon": "^1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.8 || ~6.0"
    },
    "autoload": {
        "psr-4": {
            "Louis\\Zenvia\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Louis\\Zenvia\\Test\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-develop": "1.0-dev"
        },
        "laravel": {
            "aliases": {
                "Zenvia": "Louis\\Zenvia\\Facades\\JWTAuth",
                "JWTFactory": "Tymon\\JWTAuth\\Facades\\JWTFactory"
            },
            "providers": [
                "Louis\\Zenvia\\Providers\\ZenviaServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "prefer-stable": true,
    "minimum-stability": "dev",
    "scripts": {
        "test": "phpunit --colors=always",
        "test:ci": "composer test -- --verbose --coverage-text --coverage-clover=coverage.xml"
    }
}
