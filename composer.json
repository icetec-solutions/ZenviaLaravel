{
    "name": "louisk/zenvia",
    "description": "Zenvia Package for sending SMS",
    "keywords": [
        "zenvia",
        "sms",
        "php",
        "laravel"
    ],
    "homepage": "https://github.com/keviinlouis/ZenviaLaravel",
    "support": {
        "issues": "https://github.com/keviinlouis/ZenviaLaravel/issues",
        "source": "https://github.com/keviinlouis/ZenviaLaravel"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Kevin Louis",
            "email": "keviinlouiis@gmail.com",
            "homepage": "https://github.com/keviinlouis",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.0",
        "illuminate/support": "5.1.* || 5.2.* || 5.3.* || 5.4.* || 5.5.* || 5.6.*",
        "guzzlehttp/guzzle": "~6.0",
        "nesbot/carbon": "^1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.8 || ~6.0"
    },
    "autoload": {
        "psr-4": {
            "Louis\\Zenvia\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Louis\\Zenvia\\Test\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-develop": "1.0-dev"
        },
        "laravel": {
            "aliases": {
                "Zenvia": "Louis\\Zenvia\\Facades\\ZenviaFactory"
            },
            "providers": [
                "Louis\\Zenvia\\Providers\\ZenviaServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    }
}
